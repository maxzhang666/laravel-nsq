{
  "name": "maxzhang666/laravel-nsq",
  "description": "Laravel NSQ",
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "6.*|7.*|8.*",
    "merkeleon/phpnsq": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Merkeleon\\Nsq\\NsqServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Merkeleon\\Nsq\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
